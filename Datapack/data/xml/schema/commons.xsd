<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://la2j.org"
           xmlns="http://la2j.org"
           elementFormDefault="qualified">

    <xs:simpleType name="BodyPart">
        <xs:restriction base="xs:string">
            <xs:enumeration value="RIGHT_HAND"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ItemType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="SWORD"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="Race">
        <xs:restriction base="xs:string">
            <xs:enumeration value="HUMAN"/>
            <xs:enumeration value="ELF"/>
            <xs:enumeration value="DARKELF"/>
            <xs:enumeration value="ORC"/>
            <xs:enumeration value="DWARF"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="LocationList">
        <xs:sequence>
            <xs:element name="location" type="Location" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Location">
        <xs:attribute name="x" type="xs:int" default="0"/>
        <xs:attribute name="y" type="xs:int" default="0"/>
        <xs:attribute name="z" type="xs:int" default="0"/>
    </xs:complexType>

    <xs:complexType name="Damage">
        <xs:attribute name="vertical" type="xs:int" default="0"/>
        <xs:attribute name="horizontal" type="xs:int" default="0"/>
        <xs:attribute name="distance" type="xs:int" default="0"/>
        <xs:attribute name="width" type="xs:int" default="0"/>
    </xs:complexType>

    <xs:simpleType name="Stats">
        <xs:restriction base="xs:string">
            <xs:enumeration value="MAX_HP"/>
            <xs:enumeration value="MAX_MP"/>
            <xs:enumeration value="MAX_CP"/>
            <xs:enumeration value="REGENERATE_HP_RATE"/>
            <xs:enumeration value="REGENERATE_CP_RATE"/>
            <xs:enumeration value="REGENERATE_MP_RATE"/>
            <xs:enumeration value="RECHARGE_MP_RATE"/>
